@article{Schaubel2010,
abstract = {In biomedical studies where the event of interest is recurrent (e.g., hospitalization), it is often the case that the recurrent event sequence is subject to being stopped by a terminating event (e.g., death). In comparing treatment options, the marginal recurrent event mean is frequently of interest. One major complication in the recurrent/terminal event setting is that censoring times are not known for subjects observed to die, which renders standard risk set based methods of estimation inapplicable. We propose two semiparametric methods for estimating the difference or ratio of treatment-specific marginal mean numbers of events. The first method involves imputing unobserved censoring times, while the second methods uses inverse probability of censoring weighting. In each case, imbalances in the treatment-specific covariate distributions are adjusted out through inverse probability of treatment weighting. After the imputation and/or weighting, the treatment-specific means (then their difference or ratio) are estimated nonparametrically. Large-sample properties are derived for each of the proposed estimators, with finite sample properties assessed through simulation. The proposed methods are applied to kidney transplant data.},
author = {Schaubel, Douglas E and Zhang, Min},
doi = {10.1007/s10985-009-9149-x},
file = {:home/chris/Documents/Mendeley Desktop/Schaubel, Zhang - 2010 - Estimating treatment effects on the marginal recurrent event mean in the presence of a terminating event.pdf:pdf},
issn = {1572-9249},
journal = {Lifetime data analysis},
keywords = {Age Factors,Computer Simulation,Data Interpretation, Statistical,Female,Hospitalization,Humans,Kidney Transplantation,Male,Models, Statistical,Recurrence,Tissue Donors,Treatment Outcome},
month = oct,
number = {4},
pages = {451--77},
pmid = {20063183},
title = {{Estimating treatment effects on the marginal recurrent event mean in the presence of a terminating event.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20063183},
volume = {16},
year = {2010}
}
