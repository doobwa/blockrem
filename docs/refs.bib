journal = {Lifetime data analysis},
keywords = {Clinical Trials as Topic,Computer Simulation,Cystic Fibrosis,Cystic Fibrosis: drug therapy,Cystic Fibrosis: pathology,Deoxyribonucleases,Deoxyribonucleases: therapeutic use,Humans,Models, Statistical,Recurrence},
